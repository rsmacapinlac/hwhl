# SearXNG Environment Configuration

# Secret key for SearXNG (change this to a random value)
SEARXNG_SECRET=change_this_to_a_random_secret_key

# Timezone
TZ={{ timezone | default('America/Vancouver') }}

# Base URL
SEARXNG_BASE_URL=https://searxng.{{ ansible_facts["hostname"] | default('ritchie') }}.{{ homelab_domain | default('int.macapinlac.network') }}
